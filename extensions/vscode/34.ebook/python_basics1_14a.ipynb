    "        msg['Subject'] = subject\n",
    "        \n",
    "        # HTML ë³¸ë¬¸ ì¶”ê°€\n",
    "        msg.attach(MIMEText(html_content, 'html'))\n",
    "        \n",
    "        # ì²¨ë¶€íŒŒì¼ ì¶”ê°€\n",
