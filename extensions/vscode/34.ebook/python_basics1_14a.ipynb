    "from email.mime.application import MIMEApplication\n",
    "import smtplib\n",
    "\n",
    "class EmailAutomation:\n",
    "    \"\"\"ì´ë©”ì¼ ìë™í™” í´ë˜ìŠ¤\"\"\"\n",
